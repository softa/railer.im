#teams_profile.content_wrapper
  .container
    .wrapper
      - for team in @teams
        - if team.thumbnail
          = link_to team, :style => 'text-align:center; display: inline-block; width: 256px; height: 256px; padding:4px; background:white; vertical-align:top;' do
            = image_tag team.thumbnail, :class => 'image'
            %br
            %strong= team.name
